@IsTest
public class JSON2ApexIntegration_Test {
	
	// This test method should give 100% coverage
	static testMethod void testParse() {
		String json = '{'+
		'	\"createdBy\"  : {\"name\" : \"Simon\", \"username\" :\"@superfell\" },'+
		'	\"modifiedBy\" : {\"name\" : \"Simon\", \"username\" :\"@superfell\" }'+
		'}';
		JSON2ApexIntegration r = JSON2ApexIntegration.parse(json);
		System.assert(r != null);

		json = '{\"TestAMissingObject\": { \"TestAMissingArray\": [ { \"TestAMissingProperty\": \"Some Value\" } ] } }';
		JSON2ApexIntegration.CreatedBy objCreatedBy = new JSON2ApexIntegration.CreatedBy(System.JSON.createParser(json));
		System.assert(objCreatedBy != null);
		System.assert(objCreatedBy.name == null);
		System.assert(objCreatedBy.username == null);

		json = '{\"TestAMissingObject\": { \"TestAMissingArray\": [ { \"TestAMissingProperty\": \"Some Value\" } ] } }';
		JSON2ApexIntegration objJSON2ApexIntegration = new JSON2ApexIntegration(System.JSON.createParser(json));
		System.assert(objJSON2ApexIntegration != null);
		System.assert(objJSON2ApexIntegration.createdBy == null);
		System.assert(objJSON2ApexIntegration.modifiedBy == null);
	}
}