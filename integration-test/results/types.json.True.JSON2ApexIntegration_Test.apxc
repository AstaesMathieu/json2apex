		System.assert(objOne_obj != null);
		System.assert(objOne_obj.hello == null);

		json = '{\"TestAMissingObject\": { \"TestAMissingArray\": [ { \"TestAMissingProperty\": \"Some Value\" } ] } }';
		JSON2ApexIntegration objJSON2ApexIntegration = new JSON2ApexIntegration(System.JSON.createParser(json));
		System.assert(objJSON2ApexIntegration != null);
		System.assert(objJSON2ApexIntegration.one_string == null);
		System.assert(objJSON2ApexIntegration.one_int == null);
		System.assert(objJSON2ApexIntegration.one_double == null);
		System.assert(objJSON2ApexIntegration.one_long == null);
		System.assert(objJSON2ApexIntegration.one_bool == null);
		System.assert(objJSON2ApexIntegration.one_obj == null);
		System.assert(objJSON2ApexIntegration.string_a == null);
		System.assert(objJSON2ApexIntegration.int_a == null);
		System.assert(objJSON2ApexIntegration.double_a == null);
		System.assert(objJSON2ApexIntegration.long_a == null);
		System.assert(objJSON2ApexIntegration.bool_a == null);
		System.assert(objJSON2ApexIntegration.obj_a == null);
	}
