		'	'+
		'	\"string_a\" : [ \"world\" ],'+
		'	\"int_a\": [1],'+
		'	\"double_a\" : [1.1],'+
		'	\"long_a\": [9223372036854775807],'+
		'	\"bool_a\": [true],'+
		'	\"obj_a\" : [{ \"hello\" : \"world \"}]'+
		'	'+
		'}';
		JSON2ApexIntegration r = JSON2ApexIntegration.parse(json);
		System.assert(r != null);

